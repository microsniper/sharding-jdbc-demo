<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sniper.readwritespringboot.mapper.UserMapper">

    <resultMap id="baseResultMap" type="com.sniper.shardingtablepringboot.entity.User">
        <result column="id" property="id" jdbcType="INTEGER" />
        <result column="name" property="name" jdbcType="VARCHAR" />
        <result column="age" property="age" jdbcType="INTEGER" />
        <result column="address" property="address" jdbcType="VARCHAR" />
    </resultMap>

    <select id="selectList" resultMap="baseResultMap">
        select *
        from user
    </select>

    <insert id="insert" parameterType="com.sniper.shardingtablepringboot.entity.User">
        insert into user (age,name,address) values (#{age},#{name},#{address})
    </insert>
</mapper>
